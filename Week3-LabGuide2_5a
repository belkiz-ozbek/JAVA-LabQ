#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[])
{
	int a=6;
	int b=9;
	int c=10;
	
	double e=4.5;
	double t=8.7;
	
	double result;
	

	result= (a+((c+1/e)/((b*t)/a)))/((c*e*b)/(b+1));
	
	printf("The result is:%f",result);
	
	
	return 0;
}
